<h1> List of all Locations of <%= @company.name%> </h1>
  <table>
    <thead>
      <tr>
        <th> Location </th>
        <th colspan=3> Actions </th>
      </tr>
      <tr>
    </thead>
      <tbody>
        <% @company.locations.each do |location| %>
          <tr>
        <td><%= location.place %></td>
          </tr>
        <% end %>
      </tbody>
  </table>